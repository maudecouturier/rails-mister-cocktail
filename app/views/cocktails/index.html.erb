<div class="card-category">
  <h1>Cocktails</h1>
</div>
<div class="cards">
  <% @cocktails.each do |cocktail| %>
    <div class="card-category">
      <p><%= cocktail.name %> | <%= link_to 'See recipe', cocktail_path(cocktail) %></p>
    </div>
  <% end %>
</div>

<p><%= link_to 'Add a new recipe', new_cocktail_path %></p>



